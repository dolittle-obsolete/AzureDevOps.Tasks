{
    "manifestVersion": 1,
    "id": "tasks",
    "name": "Dolittle Pipeline Tasks",
    "version": "1.0.4",
    "links": {
        "repository": {
            "uri": "https://github.com/dolittle-tools/AzureDevOps.Tasks"
        },
        "issues": {
            "uri": "https://github.com/dolittle-tools/AzureDevOps.Tasks/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/dolittle-tools/AzureDevOps.Tasks"
    },
    "publisher": "Dolittle",
    "public": false,
    "description": "Custom DevOps Pipeline Tasks for Dolittle.",
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "license": {
            "path": "LICENSE"
        },
        "details": {
            "path": "overview.md"
        }
    },
    "icons": {
        "default": "Images/dolittle_red_white_logo_256px.png"        
    },
    "files": [
        {
            "path": "node_modules"
        },
        {
            "path": "Distribution/Tasks"
        }
    ],
    "contributions": [
        {
            "id": "CalculateNewVersion",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Distribution/Tasks/CalculateNewVersion"
            }
        },
        {
            "id": "SetupBuildContext",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Distribution/Tasks/SetupBuildContext"
            }
        },
        {
            "id": "TriggerCascadingBuilds",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Distribution/Tasks/TriggerCascadingBuilds"
            }
        },
        {
            "id": "TagNewVersion",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Distribution/Tasks/TagNewVersion"
            }
        }
    ]
}